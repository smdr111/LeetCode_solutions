class Solution:
    def isValid(self, s: str) -> bool:
        result=[]
        pairs = {')':'(','}':'{',']':'['}
        for char in s:
            if char in '([{':
                result.append(char)
            else:
                if not result or result[-1] != pairs[char]:
                    return False
                result.pop()
        return not result

